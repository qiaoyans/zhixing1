var (
	status int //0 结束，1 未开始，2 执行中 3 自定义动画节点结束
)

func playAction() {
}

func playEnd() {
}

onStart => {
	status = 1

	hide
	setSize 0.01
	setXYpos 300, -300
}

onMsg "VoiceOver08Play", => {
	status = 2
	show

	playAction
	status = 3
}

onMsg "VoiceOver08End", => {
	playEnd

	hide
	status = 0
}
